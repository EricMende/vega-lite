{
  "overrides": [
    {
      "files": "*",
      "options": {
        "printWidth": 120
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript",
        "singleQuote": true,
        "bracketSpacing": false,
        "arrowParens": "avoid",
        "trailingComma": "none"
      }
    }, {
      "files": "*.md",
      "options": {
        "proseWrap": "never"
      }
    }
  ]
}
